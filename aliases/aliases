# Short aliases for Termux scripts
alias wallai='wallai.sh'
alias gpullall='githelper.sh pull-all'
alias gpushall='githelper.sh push-all'
alias gstatus='githelper.sh status'
alias gpush='githelper.sh push'
alias gclone='githelper.sh clone'
alias ghl='githelper.sh'
alias ginit='githelper.sh init'
alias gundo='githelper.sh revert-last'
alias gcloneall='githelper.sh clone-mine'
alias gnewrepo='githelper.sh newrepo'
